@{
    var user = model.user;
    var safes = user.getSafes();
}
<!DOCTYPE html>
<html lang="en">
<head>
    <title>Geeo</title>
    <script src="/scripts/jquery.js"></script>
    <script src="/scripts/main.js"></script>
    <link rel="stylesheet" href="/themes/main.css">
</head>
<body>
    <div id="main">
        <div id="main_inner">
            <div id="menu">
                <div id="inventories">

                    <div id="default" class="inventory">
                        <span class="name">default</span>
                    </div>
                    
                        @safes.forEach(function(safe){
                            <div class="inventory">
                                <span class="name" invname="@safe.safe.name">@safe.safe.name</span>
                            </div>
                        })
                    <div id="add" class="inventory">+</div>
                </div>
                <div id="user">
                    <div id="profile">
                        <img src="https://via.placeholder.com/30" />
                        <span>Username</span>
                    </div>
                </div>
            </div>
            <div id="content">
                <div id="inventory">
                
                </div>
            </div>
        </div>
    </div>
</body>
</html>