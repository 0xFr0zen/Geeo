<div id="main">
    <div id="main_inner">
        <%- include('menu');%>
        <% if(!load) { %>
        <%  incs.forEach(function(inc){ %>
        <%-     include(inc); %>
        <%  }); %>
        <% }else { %>
        <%- include('content', {load:load}); %>
        <% }%>
    </div>
</div>
<div id="prompter">
    <div id="inputholder">
        <input type="text" id="userinput" />
        <button id="ok" class="accept material-icons">check</button>
        <button id="cancel" class="cancel material-icons">close</button>
    </div>
</div>
